{
  "name": "alpha-seeker",
  "version": "1.0.0",
  "description": "Mobile-first Solana trading intelligence platform for the retail trader",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --parallel run dev",
    "build": "pnpm --parallel run build",
    "test": "pnpm --parallel run test",
    "lint": "pnpm --parallel run lint",
    "type-check": "pnpm --parallel run type-check",
    "clean": "pnpm --parallel run clean && rm -rf node_modules",
    "mobile": "pnpm --filter mobile",
    "api": "pnpm --filter api"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "react-native-paper": "^5.14.5"
  }
}