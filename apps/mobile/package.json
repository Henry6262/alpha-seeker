    "@tanstack/react-query": "^5.24.1",
    "buffer": "^6.0.3",
    "expo": "^52.0.43",
    "expo-clipboard": "~7.0.1",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.19",
